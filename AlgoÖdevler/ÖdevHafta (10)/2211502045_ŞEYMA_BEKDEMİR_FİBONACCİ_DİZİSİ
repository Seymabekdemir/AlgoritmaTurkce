import java.util.Scanner;

public class Fibonacci {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Kaç Fibonacci terimi hesaplamak istiyorsunuz?: ");
        int n = scanner.nextInt();

        System.out.println("Fibonacci Dizisi:");
        for (int i = 0; i < n; i++) {
            System.out.print(fibonacci(i) + " ");
        }

        scanner.close();
    }

    public static int fibonacci(int n) {
        // Eğer n 0 veya 1 ise, n değerini döndür
        if (n <= 1)
            return n;

        // Fibonacci dizisinin n. ve (n-1). terimlerini hesaplayıp topla
        int fibNMinus1 = 1; // (n-1). terim
        int fibNMinus2 = 0; // (n-2). terim
        int fibN = 0; // n. terim

        for (int i = 2; i <= n; i++) {
            fibN = fibNMinus1 + fibNMinus2;
            fibNMinus2 = fibNMinus1;
            fibNMinus1 = fibN;
        }

        return fibN;
    }
}
